{
  "name": "cosmosdb-query-highlighter",
  "displayName": "CosmosDB Query Highlighter",
  "description": "Brings syntax highlighting functionality for CosmosDB querying.",
  "version": "0.0.9",
  "publisher": "GreyCorbelSolutions",
  "icon": "images/marketplaceIcon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nosql",
      "aliases": ["NoSQL", "nosql"],
      "extensions": [".nosql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nosql",
      "scopeName": "source.nosql",
      "path": "./syntaxes/nosql.tmLanguage.json"
    }]
  },
  "assets": [
    "media/vsCodeExtension.gif"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RomanSebranek/nosql_highlighter.git"
  }
}
