{
  "name": "cosmosdb-query-highlighter",
  "displayName": "Cosmos DB Query Highlighter",
  "description": "Brings syntax highlighting functionality for Cosmos DB querying.",
  "version": "1.0.3",
  "publisher": "GreyCorbelSolutions",
  "icon": "images/marketplaceIcon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nosql",
      "aliases": ["NoSQL", "nosql"],
      "extensions": [".nosql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nosql",
      "scopeName": "source.nosql",
      "path": "./syntaxes/nosql.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GreyCorbel/cosmos_db_highlighter.git"
  }
}
